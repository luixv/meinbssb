{
  "cmake.ignoreCMakeListsMissing": true,
  "java.configuration.updateBuildConfiguration": "interactive",
  "github-actions.workflows.validate": true,
  "yaml.schemas": {
    "https://json.schemastore.org/github-workflow.json": "/.github/workflows/*.yml"
  },
  "github-actions.workflows.ignoreContextErrors": true,
  "yaml.validate": true,
  "yaml.customTags": [
    "!Ref",
    "!Sub",
    "!GetAtt",
    "!If",
    "!FindInMap",
    "!ImportValue",
    "!Join",
    "!Select"
  ],
  "yaml.format.enable": true,
  "yaml.trace.server": "off",
  "files.eol": "\n",
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "dart.enableSdkFormatter": true,
  "dart.enableSnippets": true,
  "dart.analysisExcludedFolders": [],
  "dart.previewFlutterUiGuides": true,
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "dart.showTodos": true,
  "dart.enableCompletionCommitCharacters": true,
  "dart.lineLength": 80,
  "dart.updateImportsOnRename": true,
  "dart.enableServerSnippets": true,
  "dart.allowAnalytics": false,
  "dart.analysisServerFolding": true,
  "dart.closingLabels": true,
  "dart.flutterSdkPath": ".fvm/versions/3.38.3",
  "dart.flutterSdkPaths": [],
  "dart.onlyAnalyzeProjectsWithOpenFiles": false,
  "dart.allowTestsOutsideTestFolder": false,
  "dart.previewLsp": true,
  "dart.useLegacyAnalyzerProtocol": false,
  "java.project.sourcePaths": []
}